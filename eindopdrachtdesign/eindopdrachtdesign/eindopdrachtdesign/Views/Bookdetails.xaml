<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eindopdrachtdesign.Views.Bookdetails">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Titel" />
            <Label Text="{Binding Title}" />
            <Label Text="publishers"/>
            <Label Text="{Binding publishers}"/>
            <Label Text="Publishdate"/>
            <Label x:Name="lbl_publishdate"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>